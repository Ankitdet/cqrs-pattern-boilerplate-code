{
  "name": "cqrs-boilerplate-common",
  "version": "1.1.10",
  "description": "Cqrs boilerplate common library",
  "private": false,
  "license": "MIT",
  "author": "ankitdetroja@gmail.com",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "tsconfig.json",
    "tsconfig.build.json"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "start:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ankitdet/cqrs-pattern-boilerplate-code"
  },
  "keywords": [
    "nestjs",
    "cqrs",
    "boilerplate",
    "domain-driven-design",
    "ddd",
    "clean-architecture",
    "api-response",
    "exception-handler",
    "interceptor",
    "middleware",
    "typescript"
  ],
  "homepage": "https://github.com/Ankitdet/cqrs-pattern-boilerplate-code#readme",
  "bugs": {
    "url": "https://github.com/ankitdetroja/cqrs-boilerplate-common/issues"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "axios": "1.13.2",
    "axios-retry": "^4.5.0",
    "bottleneck": "^2.19.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nestjs-request-context": "^4.0.0",
    "pg": "8.16.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.3",
    "strip-ansi": "7.1.2",
    "typeorm": "0.3.28",
    "uuid": "^10.0.0",
    "winston": "3.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@golevelup/ts-jest": "^0.5.6",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^11.1.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
